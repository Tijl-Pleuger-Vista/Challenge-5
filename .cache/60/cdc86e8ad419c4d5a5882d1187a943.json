\"); */\r\n/* @font-face {font-family: ourfont; src: url(assets/fonts/Dosis-Regular.ttf);} */\r\n:root{\r\n  --main: #f6f8fc;\r\n  --main-inner: #ffffff;\r\n  --text: rgb(0, 0, 0);\r\n  --sub-text: #dddddd;\r\n  --click-text: #58a6ff;\r\n  --oreo: #5c509f;\r\n  --shadow: rgb(124, 124, 124);\r\n  --main-red: white;\r\n}\r\nbody{\r\n  font-family: sans-serif;\r\n  color: var(--text);\r\n  width: 100vw;\r\n  height: 100vh;\r\n  overflow: hidden;\r\n}\r\n.row {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  flex-direction: row;\r\n}\r\n.column{\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.line{\r\n  height: 1px;\r\n  width: 100%;\r\n  background-color: #fff;\r\n}\r\n::placeholder {\r\n  color: white;\r\n}\r\n.login-box {\r\n  position: relative;\r\n  top: 50%;\r\n  left: 50%;\r\n  width: 60%;\r\n  height: 80%;\r\n  padding: 40px;\r\n  transform: translate(-50%, -50%);\r\n  background: rgba(0,0,0,.5);\r\n  box-sizing: border-box;\r\n  box-shadow: 0 15px 25px rgba(0,0,0,.6);\r\n  border-radius: 10px;\r\n}\r\n.login-box h2 {\r\n  margin: 0 0 30px;\r\n  padding: 0;\r\n  color: #fff;\r\n  text-align: center;\r\n}\r\n.login-box .user-box {\r\n  position: relative;\r\n}\r\n.login-box .user-box input {\r\n  width: 100%;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  color: #fff;\r\n  margin-bottom: 30px;\r\n  border: none;\r\n  border-bottom: 1px solid #fff;\r\n  outline: none;\r\n  background: transparent;\r\n}\r\n.login-box form a {\r\n  position: relative;\r\n  display: inline-block;\r\n  padding: 10px 20px;\r\n  color: #fff;\r\n  font-size: 16px;\r\n  text-decoration: none;\r\n  text-transform: uppercase;\r\n  overflow: hidden;\r\n  transition: .5s;\r\n  margin-top: 40px;\r\n  letter-spacing: 4px \r\n}\r\n.bg-login{\r\n  background-image: url('../../assets/content/loginbackground.png');\r\n  background-repeat: no-repeat;\r\n  background-attachment: fixed;\r\n  background-size: cover;\r\n  height: 100vh;\r\n  width: 100vw;\r\n}\r\n.bg-main{\r\n  background-image: url('../../assets/content/main.webp');\r\n  background-repeat: no-repeat;\r\n  background-attachment: fixed;\r\n  background-size: cover;\r\n  position: absolute;\r\n  margin: 0;\r\n  top: 0;\r\n  height: 100vh;\r\n  width: 100vw;\r\n}\r\n.main{\r\n  margin: 50px 100px 0 100px;\r\n  height: 100%;\r\n}\r\n.card{\r\n  background: rgba(0,0,0,.5);\r\n  box-sizing: border-box;\r\n  box-shadow: 0 15px 25px rgba(0,0,0,.6);\r\n  border-radius: 10px;\r\n}\r\n.visible{\r\n  visibility: visible;\r\n  display: block;\r\n}\r\nsection{\r\n  visibility: hidden;\r\n  display: none;\r\n  height: calc(100% - 200px);\r\n}\r\nbutton, input[type=\"submit\"], input[type=\"reset\"]{\r\n  position: relative;\r\n  display: inline-block;\r\n  padding: 10px 20px;\r\n  background: none;\r\n  color: white;\r\n  border: none;\r\n  font: inherit;\r\n  cursor: pointer;\r\n  outline: inherit;\r\n  text-decoration: none;\r\n  text-transform: uppercase;\r\n  overflow: hidden;\r\n  transition: .5s;\r\n}\r\n.button:hover{\r\n  background: var(--main-red);\r\n  color: black;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 5px var(--main-red), 0 0 25px var(--main-red), 0 0 50px var(--main-red), 0 0 100px var(--main-red);\r\n}\r\n\r\n.home-max{\r\n  height: calc(96%);\r\n}"},"lineCount":null}},"error":null,"hash":"2a3b332cf1a6999efd56c0c213522e73","cacheData":{"env":{}}}